                            </bibl>
                        </witness>
                        <witness xml:id="poems3">
                            <bibl>
                                <title>Poems, Third Series</title>
                                <author>Emily Dickinson</author>
                                <editor>Mabel Loomis Todd</editor>
                                <pubPlace>Boston</pubPlace>
                                <publisher>Little, Brown and Company</publisher>
                                <date>1896</date>
